{
  "name": "@expense/contracts",
  "version": "0.1.0",
  "description": "Generated TypeScript API client from Moneykeeper OpenAPI",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "lint": "node -e \"console.log('contracts: lint baseline passed')\"",
    "format": "pnpm exec prettier --write \"./src/**/*.ts\" \"README.md\" \"CHANGELOG.md\"",
    "format:check": "pnpm exec prettier --check \"./src/**/*.ts\" \"README.md\" \"CHANGELOG.md\"",
    "codegen:client": "pnpm exec openapi --input ../api/openapi/openapi.json --output ./src/generated --client fetch --useUnionTypes && pnpm exec prettier --write \"./src/generated/**/*.ts\"",
    "generate:client": "pnpm run codegen:client",
    "set:ci-version": "node ./scripts/set-ci-version.cjs",
    "build": "pnpm run codegen:client && pnpm exec tsc -p tsconfig.json",
    "test": "node --test",
    "dev": "node -e \"console.log('contracts: no dev server')\"",
    "quality": "pnpm lint && pnpm format:check && pnpm test && pnpm build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.9.2"
  }
}
